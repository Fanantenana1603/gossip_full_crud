<h1>Editer le commentaire</h1>



<%= form_for @comment do |f| %>

<div class="form-group">
    <label> Contenu:</label>
    <%= f.text_area :content, class:'form-control' %>
</div>

<div class="form-group">
    <button type="submit" name="button" class='btn btn-default'>Update</button>
</div>

<% end %>
